{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/desktop-portfolio",
  "framework": "angular",
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    {
      "source": "/api/angular-dev/:path*",
      "destination": "https://angular.dev/:path*"
    },
    {
      "source": "/api/fireant-api/:path*",
      "destination": "https://restv2.fireant.vn/:path*"
    },
    {
      "source": "/api/fireant-static/:path*",
      "destination": "https://static.fireant.vn/:path*"
    },
    {
      "source": "/api/fireant",
      "destination": "https://fireant.vn"
    },
    {
      "source": "/api/booking-graphql",
      "destination": "https://www.booking.com/dml/graphql"
    },
    {
      "source": "/api/booking-home",
      "destination": "https://www.booking.com"
    },
    {
      "source": "/api/booking-search",
      "destination": "https://www.booking.com/searchresults.en-gb.html"
    },
    {
      "source": "/api/tuoitre/:path*",
      "destination": "https://tuoitre.vn/:path*"
    },
    {
      "source": "/api/google-apps-script",
      "destination": "https://script.google.com/macros/s/AKfycbwRpjkR_zWjSJUNhXBHhTO5bxq18iYMaJEYvOKaQlE6v-ofJgj5xNlHc-v9RuIRXqg0EA/exec"
    },
    {
      "source": "/api/business-apps-script",
      "destination": "https://script.google.com/macros/s/AKfycbyWFdurzG4dxdIgQi3F8LDsgHJ2-k5d8_tM5tU7B0ArjOPURF1ykkqd-rg1AL9LyJp0Nw/exec"
    },
    {
      "source": "/api/dnse-api/:path*",
      "destination": "https://api.dnse.com.vn/:path*"
    },
    {
      "source": "/api/dnse/:path*",
      "destination": "https://www.dnse.com.vn/:path*"
    },
    {
      "source": "/api/securities-apps-script/:scriptId/exec",
      "destination": "https://script.google.com/macros/s/:scriptId/exec"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/angular-dev/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    },
    {
      "source": "/api/fireant-api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/api/fireant-static/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/api/fireant",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/api/booking-graphql",
